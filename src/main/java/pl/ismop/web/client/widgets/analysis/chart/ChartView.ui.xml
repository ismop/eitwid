<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:chart='urn:import:org.moxieapps.gwt.highcharts.client'>
    <ui:with type="pl.ismop.web.client.widgets.analysis.chart.ChartMessages" field="messages"/>
    <ui:style>
        .chart {
            width: 100%
            height: 250px;
        }
    </ui:style>
    <g:HTMLPanel>
        <chart:Chart ui:field="chart" />
    </g:HTMLPanel>
</ui:UiBinder>