<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
		xmlns:s="urn:import:org.gwtbootstrap3.extras.slider.client.ui">
	<ui:with type="pl.ismop.web.client.widgets.monitoring.fibre.FibreMessages" field="messages"/>
	<ui:style>
		.rightPanel {
			height: 840px;
		}
	</ui:style>
	<g:HTMLPanel>
		<b:Modal ui:field="modal" title="Pomiary światłowodowe" fade="true" width="95%">
			<b:ModalBody>
				<b:Container ui:field="container" fluid="true">
					<b:Row>
						<b:Column size="MD_9">
							<g:FlowPanel ui:field="leftPanel"/>
						</b:Column>
						<b:Column size="MD_3">
							<g:FlowPanel ui:field="rightPanel" addStyleNames="{style.rightPanel}"/>
						</b:Column>
					</b:Row>
				</b:Container>
			</b:ModalBody>
		</b:Modal>
	</g:HTMLPanel>
</ui:UiBinder>