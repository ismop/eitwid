<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="pl.ismop.web.client.widgets.experiment.ExperimentMessages" field="messages"/>
	<ui:style>
	</ui:style>
	<g:HTMLPanel ui:field="panel">
		<div class="modal fade" id="experiment-modal">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
						<h4><ui:text from="{messages.leveesExperimentModalTitle}"/></h4>
					</div>
					<div class="modal-header" id="experiment-modal-body">
						<g:Label ui:field="profileLabel"/>
						<g:ListBox ui:field="days"/>
					</div>
					<div class="modal-footer" id="experiment-modal-footer">
						<g:Button ui:field="startButton" styleName="btn btn-primary"><ui:text from="{messages.startExperimentButtonLabel}"/></g:Button>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>